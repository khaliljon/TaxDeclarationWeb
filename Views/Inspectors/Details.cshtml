@model TaxDeclarationWeb.Models.Inspector
@{
    ViewData["Title"] = "Детали инспектора";
}
<h2>@ViewData["Title"]</h2>

<div>
    <h4>@Model.FullName</h4>
    <p><strong>Код:</strong> @Model.Code</p>
    <p><strong>Инспекция:</strong> @Model.Inspection?.Name</p>
    <p><strong>Телефон:</strong> @Model.Phone</p>
</div>

<a asp-action="Index" class="btn btn-secondary">Назад</a>