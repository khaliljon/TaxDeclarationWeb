@model TaxDeclarationWeb.Models.Inspector
@{
    ViewData["Title"] = "Добавить инспектора";
}
<h2>@ViewData["Title"]</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Code"></label>
        <input asp-for="Code" class="form-control" />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="FullName"></label>
        <input asp-for="FullName" class="form-control" />
        <span asp-validation-for="FullName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="InspectionCode">Инспекция</label>
        <select asp-for="InspectionCode" class="form-control" asp-items="ViewBag.InspectionCode"></select>
        <span asp-validation-for="InspectionCode" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Phone"></label>
        <input asp-for="Phone" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
    <input type="submit" value="Сохранить" class="btn btn-primary" />
</form>